		</control>

<!--  SelectPrevTrack should be 0x39 when value = x0, off = x40 -->
<!--  SelectPrevTrack should be 0x39 when value = x7F, off = x40 -->
		<control>
			<group>[Playlist]</group>
			<key>SelectPrevTrack</key>
			<miditype>Ctrl</miditype>
			<midino>0x16</midino>
		</control>
		<control>
			<group>[Playlist]</group>
			<key>SelectNextTrack</key>
