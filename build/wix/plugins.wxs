<?xml version="1.0" encoding="utf-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
    <Fragment>
        <DirectoryRef Id="pluginsDir">
            <Directory Id="soundsource" Name="soundsource">
                <Component Id="soundsourcemediafoundation.dll" Guid="*">
                    <File Id="soundsourcemediafoundation.dll" KeyPath="yes" Source="$(var.pluginsVar)\soundsource\soundsourcemediafoundation.dll" />
                </Component>
                <Component Id="soundsourcemediafoundation.exp" Guid="*">
                    <File Id="soundsourcemediafoundation.exp" KeyPath="yes" Source="$(var.pluginsVar)\soundsource\soundsourcemediafoundation.exp" />
                </Component>
                <Component Id="soundsourcemediafoundation.lib" Guid="*">
                    <File Id="soundsourcemediafoundation.lib" KeyPath="yes" Source="$(var.pluginsVar)\soundsource\soundsourcemediafoundation.lib" />
                </Component>
            </Directory>
            <Directory Id="vamp" Name="vamp">
                <Component Id="libmixxxminimal.dll" Guid="*">
                    <File Id="libmixxxminimal.dll" KeyPath="yes" Source="$(var.pluginsVar)\vamp\libmixxxminimal.dll" />
                </Component>
                <Component Id="libmixxxminimal.exp" Guid="*">
                    <File Id="libmixxxminimal.exp" KeyPath="yes" Source="$(var.pluginsVar)\vamp\libmixxxminimal.exp" />
                </Component>
                <Component Id="libmixxxminimal.lib" Guid="*">
                    <File Id="libmixxxminimal.lib" KeyPath="yes" Source="$(var.pluginsVar)\vamp\libmixxxminimal.lib" />
                </Component>
            </Directory>
        </DirectoryRef>
    </Fragment>
    <Fragment>
        <ComponentGroup Id="pluginsComp">
            <ComponentRef Id="soundsourcemediafoundation.dll" />
            <ComponentRef Id="soundsourcemediafoundation.exp" />
            <ComponentRef Id="soundsourcemediafoundation.lib" />
            <ComponentRef Id="libmixxxminimal.dll" />
            <ComponentRef Id="libmixxxminimal.exp" />
            <ComponentRef Id="libmixxxminimal.lib" />
        </ComponentGroup>
    </Fragment>
</Wix>