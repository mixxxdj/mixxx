   			<group>[Channel1]</group>
			<key>filterLowKill</key>
			<miditype>Ctrl</miditype>
			<midino>0x10</midino>
			<options>
				<button/>
			</options>
		</control>
    		<control>
   			<group>[Channel2]</group>
			<key>filterLowKill</key>
			<miditype>Ctrl</miditype>
			<midino>0x28</midino>
			<options>
				<button/>
			</options>
		</control>

<!-- Sync Buttons -->
		<control>
			<group>[Channel1]</group>
			<key>beatsync</key>
			<miditype>Ctrl</miditype>
			<midino>0x07</midino>
			<options>
				<switch/>
			</options>
		</control>
		<control>
			<group>[Channel2]</group>
			<key>beatsync</key>
			<miditype>Ctrl</miditype>
			<midino>0x1F</midino>
			<options>
				<switch/>
			</options>
		</control>

<!-- Up/Down -->
		<control>
			<group>[Playlist]</group>
			<key>SelectPrevTrack</key>
			<miditype>Ctrl</miditype>
			<midino>0x2A</midino>
		</control>
		<control>
			<group>[Playlist]</group>
			<key>SelectNextTrack</key>
			<miditype>Ctrl</miditype>
			<midino>0x2B</midino>
		</control>

<!-- Load Deck A/B -->
		<control>
			<group>[Channel1]</group>
			<key>LoadSelectedTrack</key>
			<miditype>Ctrl</miditype>
			<midino>0x12</midino>
			<options>
				<button/>
			</options>
		</control>
		<control>
			<group>[Channel2]</group>
			<key>LoadSelectedTrack</key>
			<miditype>Ctrl</miditype>
			<midino>0x16</midino>
			<options>
				<button/>
			</options>
		</control>

<!-- A/B Gain  -->
		<control>
			<group>[Channel1]</group>
			<key>pregain</key>
			<miditype>Ctrl</miditype>
			<midino>0x33</midino>
		</control>
		<control>
			<group>[Channel2]</group>
			<key>pregain</key>
			<miditype>Ctrl</miditype>
			<midino>0x3D</midino>
		</control>
		
	</controls>

	<lights>
		<light>
			<group>[Channel1]</group>
			<key>play</key>
			<status>0xB0</status>
			<midino>0x0B</midino>
			<threshold>0.5</threshold>
		</light>
		<light>
			<group>[Channel2]</group>
			<key>play</key>
			<status>0xB0</status>
			<midino>0x23</midino>
			<threshold>0.5</threshold>
		</light>

