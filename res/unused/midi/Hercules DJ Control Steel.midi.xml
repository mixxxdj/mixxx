<!DOCTYPE controller>
<controller>
	<controls>
		<control>
			<group>[Master]</group>
			<key>crossfader</key>
			<miditype>Ctrl</miditype>
			<midino>0x39</midino>
		</control>
<!-- Pitch Bend + / - keys for DJ Control Steel -->
                <control>
                        <group>[Channel1]</group>
                        <key>rate_temp_up</key>
                        <miditype>Ctrl</miditype>
                        <midino>0x08</midino>
                        <options>
                                <switch/>
                        </options>
                </control>
                <control>
                        <group>[Channel1]</group>
                        <key>rate_temp_down</key>
                        <miditype>Ctrl</miditype>
                        <midino>0x11</midino>
                        <options>
                                <switch/>
                        </options>
                </control>
                <control>
                        <group>[Channel2]</group>
                        <key>rate_temp_up</key>
                        <miditype>Ctrl</miditype>
                        <midino>0x15</midino>
                        <options>
                                <switch/>
                        </options>
                </control>
                <control>
                        <group>[Channel2]</group>
                        <key>rate_temp_down</key>
                        <miditype>Ctrl</miditype>
                        <midino>0x20</midino>
                        <options>
                                <switch/>
                        </options>
                </control>

<!-- Scratch -->
<!-- There is no Master scratch control in Mixxx. -Albert 09/15/2008
		<control>
			<group>[Master]</group>
			<key>scratch</key>
			<miditype>Key</miditype>
			<midino>0x29</midino>
			<midichan>1</midichan>
		</control>
-->
<!-- Balance -->
		<control>
			<group>[Master]</group>
			<key>balance</key>
			<miditype>Ctrl</miditype>
			<midino>0x37</midino>
		</control>

<!-- Main Volume -->
		<control>
			<group>[Master]</group>
			<key>volume</key>
			<miditype>Ctrl</miditype>
			<midino>0x38</midino>
		</control>
		
<!-- Left Deck = Channel1, Right Deck = Channel2 -->
		<control>
			<group>[Channel1]</group>
			<key>flanger</key>
			<miditype>Ctrl</miditype>
			<midino>0x01</midino>
			<options>
				<button/>
			</options>
		</control>
		<control>
			<group>[Channel1]</group>
			<key>reverse</key>
			<miditype>Ctrl</miditype>
			<midino>0x04</midino>
			<options>
				<switch/>
			</options>
		</control>
		<control>
			<group>[Channel2]</group>
			<key>flanger</key>
			<miditype>Ctrl</miditype>
			<midino>0x19</midino>
			<options>
				<button/>
			</options>
		</control>
		<control>
			<group>[Channel2]</group>
			<key>reverse</key>
			<miditype>Ctrl</miditype>
			<midino>0x1C</midino>
			<options>
				<switch/>
			</options>
		</control>
		<control>
			<group>[Channel1]</group>
			<key>volume</key>
			<miditype>Ctrl</miditype>
			<midino>0x32</midino>
		</control>
		<control>
			<group>[Channel1]</group>
			<key>filterHigh</key>
			<miditype>Ctrl</miditype>
			<midino>0x34</midino>
		</control>
		<control>
			<group>[Channel1]</group>
			<key>filterMid</key>
			<miditype>Ctrl</miditype>
			<midino>0x35</midino>
		</control>
		<control>
			<group>[Channel1]</group>
			<key>filterLow</key>
			<miditype>Ctrl</miditype>
			<midino>0x36</midino>
		</control>
		<control>
			<group>[Channel1]</group>
			<key>rate</key>
			<miditype>Ctrl</miditype>
			<midino>0x31</midino>
		</control>
		<control>
			<group>[Channel1]</group>
			<key>jog</key>
			<miditype>Ctrl</miditype>
			<midino>0x2F</midino>
			<options>
				<hercjog/>
			</options>
		</control>
		<control>
			<group>[Channel1]</group>
			<key>play</key>
			<miditype>Ctrl</miditype>
			<midino>0x0B</midino>
			<options>
				<button/>
			</options>
		</control>
		<control>
			<group>[Channel1]</group>
			<key>cue_default</key>
			<miditype>Ctrl</miditype>
			<midino>0x0C</midino>
			<options>
				<button/>
			</options>
		</control>
		<control>
			<group>[Channel1]</group>
			<key>pfl</key>
			<miditype>Ctrl</miditype>
			<midino>0x14</midino>
			<options>
				<button/>
			</options>
		</control>
		<control>
			<group>[Channel1]</group>
			<key>PrevTrack</key>
			<miditype>Ctrl</miditype>
			<midino>0x09</midino>
		</control>
		<control>
			<group>[Channel1]</group>
			<key>NextTrack</key>
			<miditype>Ctrl</miditype>
			<midino>0x0A</midino>
		</control>
		
		<control>
			<group>[Channel2]</group>
			<key>volume</key>
			<miditype>Ctrl</miditype>
			<midino>0x3C</midino>
		</control>
		<control>
			<group>[Channel2]</group>
			<key>filterHigh</key>
			<miditype>Ctrl</miditype>
			<midino>0x3E</midino>
		</control>
		<control>
			<group>[Channel2]</group>
			<key>filterMid</key>
			<miditype>Ctrl</miditype>
			<midino>0x3F</midino>
		</control>
		<control>
			<group>[Channel2]</group>
			<key>filterLow</key>
			<miditype>Ctrl</miditype>
			<midino>0x40</midino>
		</control>
		<control>
			<group>[Channel2]</group>
			<key>rate</key>
			<miditype>Ctrl</miditype>
			<midino>0x3B</midino>
		</control>
		<control>
			<group>[Channel2]</group>
			<key>jog</key>
			<miditype>Ctrl</miditype>
			<midino>0x30</midino>
			<options>
				<hercjog/>
			</options>
		</control>
		<control>
			<group>[Channel2]</group>
			<key>play</key>
			<miditype>Ctrl</miditype>
			<midino>0x23</midino>
			<options>
				<button/>
			</options>
		</control>
		<control>
			<group>[Channel2]</group>
			<key>cue_default</key>
			<miditype>Ctrl</miditype>
			<midino>0x24</midino>
			<options>
				<button/>
			</options>
		</control>
		<control>
			<group>[Channel2]</group>
			<key>pfl</key>
			<miditype>Ctrl</miditype>
			<midino>0x18</midino>
			<options>
				<button/>
			</options>
		</control>
		<control>
			<group>[Channel2]</group>
			<key>PrevTrack</key>
			<miditype>Ctrl</miditype>
			<midino>0x21</midino>
		</control>
		<control>
			<group>[Channel2]</group>
			<key>NextTrack</key>
			<miditype>Ctrl</miditype>
			<midino>0x22</midino>
		</control>

<!-- New for RMX -->
<!-- Kill Buttons -->
    		<control>
   			<group>[Channel1]</group>
