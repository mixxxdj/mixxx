<Template>
	<WidgetGroup>
		<Size>0e,5f</Size>
		<Layout>horizontal</Layout>
		<Children>
			<WidgetGroup>
				<Size>0e,5f</Size>
				<Style>QGroupBox { 
					border: 0px solid green;  
					margin: 0px 0px 0px 0px;
				} 
				</Style>
				<BackPath>style/style_bg_looping.png</BackPath>
