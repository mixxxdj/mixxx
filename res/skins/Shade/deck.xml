<Template>
  <WidgetGroup>
    <ObjectName>Deck</ObjectName>
    <Size>0e,165f</Size>
    <Layout>horizontal</Layout>
    <Style>QGroupBox { border: 0px solid red; } QWidget { margin: 0; padding: 0; }</Style>
    <Children>
      <WidgetGroup>
        <ObjectName>DeckLeftBorder</ObjectName>
        <Size>5f,165f</Size>
        <BackPath>style/style_bg_deck_border.png</BackPath>
        <Style>QGroupBox { border: 0px solid red; } QWidget { margin: 10px; padding: 0; }</Style>
      </WidgetGroup>
      <WidgetGroup>
        <ObjectName>DeckLeft</ObjectName>
        <Size>0e,165f</Size>
        <Layout>vertical</Layout>
        <Style>QGroupBox { border: 0px solid red; } QWidget { margin: 0; padding: 0; }</Style>
        <Children>
          <WidgetGroup>
            <ObjectName>DeckTopLeftMidPart</ObjectName>
            <Size>0e,45f</Size>
            <Layout>horizontal</Layout>
            <Style>QGroupBox { border: 0px solid red; } QWidget { margin: 0; padding: 0; }</Style>
            <Children>
              <WidgetGroup>
                <ObjectName>DeckTopLeftPart</ObjectName>
                <Size>40f,45f</Size>
                <BackPath>style/style_bg_deck_top_left.png</BackPath>
                <Style>QGroupBox { border: 0px solid red; } QWidget { margin: 0; padding: 0; }</Style>
                <Children>
                  <CoverArt>
                    <Pos>2,7</Pos>
                    <TooltipId>coverart</TooltipId>
                    <ObjectName>DeckCoverArt</ObjectName>
                    <Size>35f,35f</Size>
                    <Group>[Channel<Variable name="channum"/>]</Group>
                  </CoverArt>
                </Children>
              </WidgetGroup>
              <WidgetGroup>
                <ObjectName>DeckTopMidPart</ObjectName>
                <Size>i,45f</Size>
                <Layout>vertical</Layout>
                <BackPath>style/style_bg_deck_top_mid.png</BackPath>
                <Style>QGroupBox { border: 0px solid red; } QWidget { margin: 0; padding: 0; }</Style>
                <Children>
                  <TrackProperty>
                    <TooltipId>track_title</TooltipId>
                    <Style>QLabel {
                      font: bold 13px sans-serif;
                      font-family: "Open Sans";
                      background-color: transparent;
                      color: #191F24;
                      text-align: left;
                      padding-left: 1px;
                      padding-top: 4px;
                    }
                    </Style>
                    <Property>title</Property>
                    <Channel><Variable name="channum"/></Channel>
                    <Size>0e,20f</Size>
                    <Elide>right</Elide>
                  </TrackProperty>

                  <WidgetGroup>
                    <ObjectName>ArtistAndTimeRow</ObjectName>
                    <Size>0e,20f</Size>
                    <Layout>horizontal</Layout>
                    <Children>
                      <TrackProperty>
                        <TooltipId>track_artist</TooltipId>
                        <Style>QLabel {
                          font: bold 13px sans-serif;
                          font-family: "Open Sans";
                          background-color: transparent;
                          color: #191F24;
                          text-align: left;
                          padding-left: 1px;
                          padding-top: 4px;
                        }
                        </Style>
                        <Property>artist</Property>
                        <Channel><Variable name="channum"/></Channel>
                        <SizePolicy>me,max</SizePolicy>
                        <Elide>right</Elide>
                      </TrackProperty>
                      <NumberPos>
                        <TooltipId>track_time</TooltipId>
                        <Style>QLabel {
                          font: bold 13px sans-serif;
                          font-family: Open Sans;
                          background-color: transparent;
                          color: #191F24;
                          text-align: right;
                          padding-left: 1px;
                          padding-top: 4px;
                        }
                        </Style>
                        <Group>[Channel<Variable name="channum"/>]</Group>
                        <Alignment>right</Alignment>
                      </NumberPos>
                    </Children>
                  </WidgetGroup>
                </Children>
              </WidgetGroup>
            </Children>
            </WidgetGroup>
          <WidgetGroup>
            <ObjectName>DeckUpperLeftMidPart</ObjectName>
            <Size>0e,81f</Size>
            <Layout>horizontal</Layout>
            <Style>QGroupBox { border: 0px solid red; } QWidget { margin: 0; padding: 0; }</Style>
            <Children>
              <WidgetGroup>
                <ObjectName>DeckUpperMidPart</ObjectName>
                <Size>0e,81f</Size>
                <BackPath>style/style_bg_deck_pane.png</BackPath>
                <Layout>horizontal</Layout>
                <Style>QGroupBox { border: 0px solid red; } QWidget { margin: 0; padding: 0; }</Style>
                <Children>
                  <!-- Collapsing Waveform, VinylControl & spinning Vinyl widget-->
                  <WidgetGroup>
                    <ObjectName>DeckWaveformVinylControlSpinny</ObjectName>
                    <Size>0e,81f</Size>
                    <Layout>horizontal</Layout>
                    <Style>QGroupBox { border: 0px solid yellow; } QWidget { margin: 0; padding: 0; }</Style>
                    <Children>
                      <!--If you want the waveforms center to adjust when resizing in a collapsing widget
                      (e.g. to display spinning vinyl widget like in this skin) don't put the Waveform in a widget group,
                      and don't specify the Waveform's <Size>. So you would have:
                      Parent WidgetGroup
                        Waveform Widget (no Widgetgroup)
                        Child WidgetGroup for Spinny
                          Spinny Widget
                      -->

                      <!--
                      **********************************************
                      Visual- Waveform
                      **********************************************
                      -->
                      <Visual>
                        <TooltipId>waveform_display</TooltipId>
                        <Style></Style>
                        <Channel><Variable name="channum"/></Channel>
                        <Size>0e,81f</Size>
                        <!--See comment about <Size> above-->
                        <!--<Size>415,119</Size>-->
                        <BgColor>#8D98A3</BgColor>
                        <!--<BgPixmap>style/style_bg_waveform.png</BgPixmap>-->
                        <SignalHighColor></SignalHighColor>
                        <SignalMidColor></SignalMidColor>
                        <SignalLowColor></SignalLowColor>
                        <SignalColor>#191F24</SignalColor>
                        <BeatColor>#FFFFFF</BeatColor>
                        <PlayPosColor>#00FF00</PlayPosColor>
                        <EndOfTrackColor>#EA0000</EndOfTrackColor>
                        <AxesColor>#00FF00</AxesColor>
                        <Align></Align>
                        <DefaultMark>
                          <Align>top</Align>
                          <Color>#FD0564</Color>
                          <TextColor>#FFFFFF</TextColor>
                          <Text> %1 </Text>
                        </DefaultMark>
                        <MarkRange>
                          <StartControl>loop_start_position</StartControl>
                          <EndControl>loop_end_position</EndControl>
                          <EnabledControl>loop_enabled</EnabledControl>
                          <Color>#00FF00</Color>
                          <DisabledColor>#BCDBFB</DisabledColor>
                        </MarkRange>
                        <Mark>
                          <Control>loop_start_position</Control>
                          <Text>IN</Text>
                          <Align>bottom</Align>
                          <Color>#00FF00</Color>
                          <TextColor>#FFFFFF</TextColor>
                        </Mark>
                        <Mark>
                          <Control>loop_end_position</Control>
                          <Text>OUT</Text>
                          <Align>bottom</Align>
                          <Color>#00FF00</Color>
                          <TextColor>#FFFFFF</TextColor>
                        </Mark>
                        <!--
                        The hotcues not represented by a button in the current skin
                        show only in the waveform under two circumstances:
                        - if a MIDI device which supports more hotcues than buttons
                          are in the current skin has them activated
                        - if you change from a skin which supports more hotcues
                          than buttons are in the current skin (and has them activated)
                        -->
                        <Mark>
                          <Control>cue_point</Control>
                          <Text>CUE</Text>
                          <Align>top</Align>
                          <Color>#FF001C</Color>
                          <TextColor>#FFFFFF</TextColor>
                        </Mark>
                      </Visual>

                      <!-- Spinning Vinyl & VinylControl -->
                      <WidgetGroup>
                        <Layout>horizontal</Layout>
                        <Children>
                          <!-- Spinning Vinyl sub-widget -->
                          <WidgetGroup>
                            <Size>81f,81f</Size>
                            <Children>
                              <!--
                              **********************************************
                              Visual- Spinning vinyl
                              **********************************************
                              -->
                              <Spinny>
                                <TooltipId>spinny</TooltipId>
                                <Channel><Variable name="channum"/></Channel>
                                <PathBackground>vinyl_spinny_background.png</PathBackground>
                                <PathForeground>vinyl_spinny_foreground.png</PathForeground>
                                <PathGhost>vinyl_spinny_foreground_ghost.png</PathGhost>
                              </Spinny>
                            </Children>
                            <Connection>
                              <ConfigKey>[Spinny<Variable name="channum"/>],show_spinny</ConfigKey>
                              <BindProperty>visible</BindProperty>
                            </Connection>
                          </WidgetGroup>
                          <Template src="skin:vinylcontrol.xml">
                            <SetVariable name="channum"><Variable name="channum"/></SetVariable>
                          </Template>
                        </Children>
                      </WidgetGroup>

                    </Children>
                  </WidgetGroup>
                </Children>
              </WidgetGroup>
            </Children>
          </WidgetGroup>
          <WidgetGroup>
            <ObjectName>DeckLowerLeftMidPart</ObjectName>
            <Size>0e,39f</Size>
            <Layout>horizontal</Layout>
            <Style>QGroupBox { border: 0px solid red; } QWidget { margin: 0; padding: 0; }</Style>
            <Children>
              <WidgetGroup>
                <ObjectName>DeckLowerMidPart</ObjectName>
                <Size>0e,39f</Size>
                <Layout>horizontal</Layout>
                <BackPath>style/style_bg_deck_woverview.png</BackPath>
                <Style>QGroupBox { border: 0px solid red; } QWidget { margin: 0; padding: 0; }</Style>
                <Children>
                  <Overview>
                    <TooltipId>waveform_overview</TooltipId>
                    <Style></Style>
                    <Channel><Variable name="channum"/></Channel>
                    <Size>0e,37f</Size>
                    <!-- transparent background if no <BgColor> & <BgPixmap> is present -->
                    <BgColor></BgColor>
                    <BgPixmap></BgPixmap>
                    <SignalHighColor></SignalHighColor>
                    <SignalMidColor></SignalMidColor>
                    <SignalLowColor></SignalLowColor>
                    <SignalColor>#191F24</SignalColor>
                    <PlayPosColor>#00FF00</PlayPosColor>
                    <EndOfTrackColor>#EA0000</EndOfTrackColor>
                    <PlayedOverlayColor>#60000000</PlayedOverlayColor>
                    <DefaultMark>
                      <Align>bottom|right</Align>
                      <Color>#FD0564</Color>
                      <TextColor>#FFFFFF</TextColor>
                      <Text> %1 </Text>
                    </DefaultMark>
                    <MarkRange>
                      <StartControl>loop_start_position</StartControl>
                      <EndControl>loop_end_position</EndControl>
                      <EnabledControl>loop_enabled</EnabledControl>
                      <Color>#00FF00</Color>
                      <DisabledColor>#FFFFFF</DisabledColor>
                    </MarkRange>
                    <Mark>
                      <Control>cue_point</Control>
                      <Text>C</Text>
                      <Align>top|right</Align>
                      <Color>#FF001C</Color>
                      <TextColor>#FFFFFF</TextColor>
                    </Mark>
                    <Connection>
                      <ConfigKey>[Channel<Variable name="channum"/>],playposition</ConfigKey>
                      <EmitOnDownPress>false</EmitOnDownPress>
                    </Connection>
                  </Overview>
                </Children>
              </WidgetGroup>
              <WidgetGroup>
                <ObjectName>DeckLowerLeftPart</ObjectName>
                <Size>63f,39f</Size>
                <BackPath>style/style_bg_deck_woverview_buttons.png</BackPath>
                <Style>QGroupBox { border: 0px solid red; } QWidget { margin: 0; padding: 0; }</Style>
                <Children>
                  <PushButton>
                    <TooltipId>show_spinny</TooltipId>
                    <Style></Style>
                    <NumberStates>2</NumberStates>
                    <State>
                      <Number>0</Number>
                      <Pressed>skin:/btn/btn_spinny.png</Pressed>
                      <Unpressed>skin:/btn/btn_spinny.png</Unpressed>
                    </State>
                    <State>
                      <Number>1</Number>
                      <Pressed>skin:/btn/btn_spinny_over.png</Pressed>
                      <Unpressed>skin:/btn/btn_spinny_over.png</Unpressed>
                    </State>
                    <Pos>1,1</Pos>
                    <Connection>
                      <ConfigKey>[Spinny<Variable name="channum"/>],show_spinny</ConfigKey>
                      <EmitOnPressAndRelease>true</EmitOnPressAndRelease>
                      <ButtonState>LeftButton</ButtonState>
                    </Connection>
                  </PushButton>
                  <PushButton>
                    <TooltipId>repeat</TooltipId>
                    <Style></Style>
                    <NumberStates>2</NumberStates>
                    <State>
                      <Number>0</Number>
                      <Pressed>skin:/btn/btn_repeat.png</Pressed>
                      <Unpressed>skin:/btn/btn_repeat.png</Unpressed>
                    </State>
                    <State>
                      <Number>1</Number>
                      <Pressed>skin:/btn/btn_repeat_over.png</Pressed>
                      <Unpressed>skin:/btn/btn_repeat_over.png</Unpressed>
                    </State>
                    <Pos>22,1</Pos>
                    <Connection>
                      <ConfigKey>[Channel<Variable name="channum"/>],repeat</ConfigKey>
                    </Connection>
                  </PushButton>
                  <PushButton>
                    <TooltipId>eject</TooltipId>
                    <Style></Style>
                    <NumberStates>1</NumberStates>
                    <State>
                      <Number>0</Number>
                      <Pressed>skin:/btn/btn_eject_over.png</Pressed>
                      <Unpressed>skin:/btn/btn_eject.png</Unpressed>
                    </State>
                    <Pos>43,1</Pos>
                    <Connection>
                      <ConfigKey>[Channel<Variable name="channum"/>],eject</ConfigKey>
                      <EmitOnPressAndRelease>true</EmitOnPressAndRelease>
                      <ButtonState>LeftButton</ButtonState>
                      <ConnectValueToWidget>false</ConnectValueToWidget>
                    </Connection>
                  </PushButton>
                  <PushButton>
                    <TooltipId>beats_translate_curpos</TooltipId>
                    <Style></Style>
                    <NumberStates>1</NumberStates>
                    <State>
                      <Number>0</Number>
                      <Pressed>skin:/btn/btn_beatgrid_over.png</Pressed>
                      <Unpressed>skin:/btn/btn_beatgrid.png</Unpressed>
                    </State>
                    <Pos>1,20</Pos>
                    <Connection>
                      <ConfigKey>[Channel<Variable name="channum"/>],beats_translate_curpos</ConfigKey>
                      <EmitOnPressAndRelease>true</EmitOnPressAndRelease>
                      <ButtonState>LeftButton</ButtonState>
                    </Connection>
                    <Connection>
                      <ConfigKey>[Channel<Variable name="channum"/>],beats_translate_match_alignment</ConfigKey>
                      <EmitOnPressAndRelease>true</EmitOnPressAndRelease>
                      <ButtonState>RightButton</ButtonState>
                    </Connection>
                  </PushButton>

                  <PushButton>
                    <TooltipId>quantize</TooltipId>
                    <Style></Style>
                    <NumberStates>2</NumberStates>
                    <State>
                      <Number>0</Number>
                      <Pressed>skin:/btn/btn_quantize.png</Pressed>
                      <Unpressed>skin:/btn/btn_quantize.png</Unpressed>
                    </State>
                    <State>
                      <Number>1</Number>
                      <Pressed>skin:/btn/btn_quantize_over.png</Pressed>
                      <Unpressed>skin:/btn/btn_quantize_over.png</Unpressed>
                    </State>
                    <Pos>22,20</Pos>
                    <Connection>
                      <ConfigKey>[Channel<Variable name="channum"/>],quantize</ConfigKey>
                      <EmitOnPressAndRelease>true</EmitOnPressAndRelease>
                      <ButtonState>LeftButton</ButtonState>
                    </Connection>
                  </PushButton>
                  <PushButton>
                    <TooltipId>keylock</TooltipId>
                    <Style></Style>
                    <NumberStates>2</NumberStates>
                    <State>
                      <Number>0</Number>
                      <Pressed>skin:/btn/btn_keylock.png</Pressed>
                      <Unpressed>skin:/btn/btn_keylock.png</Unpressed>
                    </State>
                    <State>
                      <Number>1</Number>
                      <Pressed>skin:/btn/btn_keylock_over.png</Pressed>
                      <Unpressed>skin:/btn/btn_keylock_over.png</Unpressed>
                    </State>
                    <Pos>43,20</Pos>
                    <Connection>
                      <ConfigKey>[Channel<Variable name="channum"/>],keylock</ConfigKey>
                    </Connection>
                  </PushButton>

                </Children>
              </WidgetGroup>
            </Children>
          </WidgetGroup>
        </Children>
      </WidgetGroup>
      <WidgetGroup>
        <ObjectName>DeckRightPart</ObjectName>
        <Size>52f,165f</Size>
        <BackPath>style/style_bg_deck_right.png</BackPath>
        <Style>QGroupBox { border: 0px solid red; } QWidget { margin: 10px; padding: 0; }</Style>
        <Children>
          <PushButton>
            <TooltipId>sync_enabled</TooltipId>
            <Style></Style>
            <NumberStates>2</NumberStates>
            <State>
              <Number>0</Number>
              <Pressed>skin:/btn/btn_kill_down.png</Pressed>
              <Unpressed>skin:/btn/btn_kill.png</Unpressed>
            </State>
            <State>
              <Number>1</Number>
              <Pressed>skin:/btn/btn_kill_overdown.png</Pressed>
              <Unpressed>skin:/btn/btn_kill_over.png</Unpressed>
            </State>
            <Pos>6,21</Pos>
            <Connection>
              <ConfigKey>[Channel<Variable name="channum"/>],sync_enabled</ConfigKey>
              <ButtonState>LeftButton</ButtonState>
            </Connection>
            <Connection>
              <ConfigKey>[Channel<Variable name="channum"/>],beatsync_phase</ConfigKey>
              <ButtonState>RightButton</ButtonState>
            </Connection>
          </PushButton>

          <PushButton>
            <TooltipId>sync_reset_key</TooltipId>
            <Style></Style>
            <NumberStates>2</NumberStates>
            <State>
              <Number>0</Number>
              <Pressed>skin:/btn/btn_kill_down.png</Pressed>
              <Unpressed>skin:/btn/btn_kill.png</Unpressed>
            </State>
            <State>
              <Number>1</Number>
              <Pressed>skin:/btn/btn_kill_overdown.png</Pressed>
              <Unpressed>skin:/btn/btn_kill_over.png</Unpressed>
            </State>
            <Pos>32,21</Pos>
            <Connection>
              <ConfigKey>[Channel<Variable name="channum"/>],sync_key</ConfigKey>
              <ButtonState>LeftButton</ButtonState>
            </Connection>
            <Connection>
              <ConfigKey>[Channel<Variable name="channum"/>],reset_key</ConfigKey>
              <ButtonState>RightButton</ButtonState>
            </Connection>
          </PushButton>


          <WidgetGroup>
            <Pos>3,34</Pos>
            <Size>47f,26f</Size>
            <Style>QGroupBox { border: 0px solid red; } QWidget { margin: 0px; padding: 0; }
              QLabel {
                font: bold 9px sans-serif;
                font-family: "Open Sans";
                background-color: transparent;
                color: #191F24;
                padding-bottom: -2px;
              }
            </Style>
            <Layout>vertical</Layout>
            <Children>
              <NumberBpm>
                <TooltipId>visual_bpm</TooltipId>
                <Channel><Variable name="channum"/></Channel>
                <!--CSS alignments not respected see bug lp:605530 , lets call <Alignment>-->
                <Alignment>center</Alignment>
                <Connection>
                  <ConfigKey>[Channel<Variable name="channum"/>],visual_bpm</ConfigKey>
                </Connection>
              </NumberBpm>
              <Key>
                <TooltipId>visual_key</TooltipId>
                <Channel><Variable name="channum"/></Channel>
                <!--CSS alignments not respected see bug lp:605530 , lets call <Alignment>-->
                <Alignment>center</Alignment>
                <Connection>
                  <ConfigKey>[Channel<Variable name="channum"/>],visual_key</ConfigKey>
                </Connection>
              </Key>

            </Children>
          </WidgetGroup>
          <SliderComposed>
            <Style></Style>
            <Handle>knob_visual_key.png</Handle>
            <Slider>slider_visual_key.png</Slider>
            <Pos>18,59</Pos>
            <Horizontal>true</Horizontal>
            <Connection>
              <ConfigKey>[Channel<Variable name="channum"/>],visual_key_distance</ConfigKey>
              <EmitOnDownPress>false</EmitOnDownPress>
            </Connection>
          </SliderComposed>

          <PushButton>
            <TooltipId>pitch_down</TooltipId>
            <Size>10f,10f</Size>
            <Style></Style>
            <NumberStates>1</NumberStates>
            <State>
              <Number>0</Number>
              <Text>◄</Text>
            </State>
            <Pos>5,57</Pos>
            <Connection>
              <ConfigKey>[Channel<Variable name="channum"/>],pitch_down</ConfigKey>
              <ButtonState>LeftButton</ButtonState>
            </Connection>
            <Connection>
              <ConfigKey>[Channel<Variable name="channum"/>],pitch_down_small</ConfigKey>
              <ButtonState>RightButton</ButtonState>
            </Connection>
          </PushButton>

          <PushButton>
            <TooltipId>pitch_up</TooltipId>
            <Size>10f,10f</Size>
            <Style></Style>
            <NumberStates>1</NumberStates>
            <State>
              <Number>0</Number>
              <Text>►</Text>
            </State>
            <Pos>38,57</Pos>
            <Connection>
              <ConfigKey>[Channel<Variable name="channum"/>],pitch_up</ConfigKey>
              <ButtonState>LeftButton</ButtonState>
            </Connection>
            <Connection>
              <ConfigKey>[Channel<Variable name="channum"/>],pitch_up_small</ConfigKey>
              <ButtonState>RightButton</ButtonState>
            </Connection>
          </PushButton>

          <SliderComposed>
            <TooltipId>pitch</TooltipId>
            <Style></Style>
            <Handle>knob_visual_key.png</Handle>
            <Slider>slider_visual_key.png</Slider>
            <Pos>18,63</Pos>
            <Horizontal>true</Horizontal>
            <Connection>
              <ConfigKey>[Channel<Variable name="channum"/>],pitch</ConfigKey>
              <EmitOnDownPress>false</EmitOnDownPress>
            </Connection>
          </SliderComposed>

          <SliderComposed>
            <TooltipId>rate</TooltipId>
            <Style></Style>
            <Handle>knob_pitch.png</Handle>
            <Slider>slider_pitch.png</Slider>
            <Pos>20,67</Pos>
            <Horizontal>false</Horizontal>
            <Connection>
              <ConfigKey>[Channel<Variable name="channum"/>],rate</ConfigKey>
              <EmitOnDownPress>false</EmitOnDownPress>
            </Connection>
          </SliderComposed>

          <PushButton>
            <TooltipId>rate_temp_up_rate_temp_up_small</TooltipId>
            <Size>10f,10f</Size>
            <NumberStates>1</NumberStates>
            <State>
              <Number>0</Number>
              <Text>►</Text>
            </State>
            <Pos>36,148</Pos>
            <Connection>
              <ConfigKey>[Channel<Variable name="channum"/>],rate_temp_up</ConfigKey>
              <EmitOnPressAndRelease>true</EmitOnPressAndRelease>
              <ButtonState>LeftButton</ButtonState>
            </Connection>
            <Connection>
              <ConfigKey>[Channel<Variable name="channum"/>],rate_temp_up_small</ConfigKey>
              <EmitOnPressAndRelease>true</EmitOnPressAndRelease>
              <ButtonState>RightButton</ButtonState>
            </Connection>
          </PushButton>

          <PushButton>
            <TooltipId>rate_temp_down_rate_temp_down_small</TooltipId>
            <Size>10f,10f</Size>
            <NumberStates>1</NumberStates>
            <State>
              <Number>0</Number>
              <Text>◄</Text>
            </State>
            <Pos>7,148</Pos>
            <Connection>
              <ConfigKey>[Channel<Variable name="channum"/>],rate_temp_down</ConfigKey>
              <EmitOnPressAndRelease>true</EmitOnPressAndRelease>
              <ButtonState>LeftButton</ButtonState>
            </Connection>
            <Connection>
              <ConfigKey>[Channel<Variable name="channum"/>],rate_temp_down_small</ConfigKey>
              <EmitOnPressAndRelease>true</EmitOnPressAndRelease>
              <ButtonState>RightButton</ButtonState>
            </Connection>
          </PushButton>
        </Children>
      </WidgetGroup>
      <WidgetGroup>
        <ObjectName>DeckLeftBorder</ObjectName>
        <Size>5f,201f</Size>
        <BackPath>style/style_bg_deck_border.png</BackPath>
        <Style>QGroupBox { border: 0px solid red; } QWidget { margin: 10px; padding: 0; }</Style>
      </WidgetGroup>
    </Children>
  </WidgetGroup>
</Template>
