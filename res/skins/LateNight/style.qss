  #SkinSettingsSeparator {
    margin: 0px 4px 4px 4px;
  }
  QMenu::item {
    padding: 5px 13px 5px 20px;
  }
  QMenu QCheckBox {
    padding: 2px 10px 2px 3px;
  }
  QMenu QCheckBox::indicator {
    width: 13px;
    height: 13px;
    border: 1px solid #333;
    border-radius: 1px;
