/*********** library search bar *********************************/
WSearchLineEdit {
  padding: 2px;
  selection-color: #000;
  selection-background-color: #ccc;
  }
  WSearchLineEdit:focus {
    padding: 1px;
    border: 2px solid #257b82;
    border-radius: 0px;
  }
  WSearchLineEdit:disabled {
    background-color: #161617;
