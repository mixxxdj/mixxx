<!DOCTYPE LADSPASkin>
<LADSPASkin>

 <Background>
  <Path>ladspa.png</Path>
 </Background>

 <PresetList>
  <Pos>5,5</Pos>
  <Size>145,-10</Size>
 </PresetList>

 <SlotTable>
  <Slots>3</Slots>
  <Pos>150,0</Pos>
  <Size>-150,0</Size>

  <Slot>
   <Pos>5,0</Pos>
   <Size>-10,90</Size>
   <Spacing>0,4</Spacing>

   <PushButton>
    <Tooltip>Remove effect</Tooltip>
    <NumberStates>1</NumberStates>
    <State>
     <Number>0</Number>
     <Pressed>ladspaXon.png</Pressed>
     <Unpressed>ladspaXoff.png</Unpressed>
    </State>
    <Pos>308,5</Pos>
    <Connection>
     <ConfigKey>[LADSPA],RemoveEffect</ConfigKey>
    </Connection>
   </PushButton>

   <PushButton>
    <Tooltip>Effect on/off</Tooltip>
    <NumberStates>2</NumberStates>
    <State>
     <Number>0</Number>
     <Pressed>ladspaoff.png</Pressed>
     <Unpressed>ladspaoff.png</Unpressed>
    </State>
    <State>
     <Number>1</Number>
     <Pressed>ladspaon.png</Pressed>
     <Unpressed>ladspaon.png</Unpressed>
    </State>
    <Pos>276,5</Pos>
    <Connection>
     <ConfigKey>[LADSPA],EnableEffect</ConfigKey>
    </Connection>
   </PushButton>

   <Label>
    <Pos>10,10</Pos>
    <Width>250</Width>
   </Label>

   <Knob>
    <Tooltip>A knob</Tooltip>
    <NumberStates>41</NumberStates>
    <Path>knobm%1.png</Path>
    <Pos>20,30</Pos>
    <Spacing>20,0</Spacing>
    <Connection>
     <ConfigKey>[LADSPA],None</ConfigKey>
    </Connection>
   </Knob>
  </Slot>
 </SlotTable>

 <BgColor>#ffffff</BgColor>
 <FgColor>#000000</FgColor>
</LADSPASkin>
