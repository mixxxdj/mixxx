<!DOCTYPE template>
<!--
  Description:
    An effect meta knob.
  Variables:
    control: The control to connect to.
-->
<Template>
  <WidgetGroup>
    <ObjectName>EffectMetaKnob</ObjectName>
    <Layout>vertical</Layout>
    <Size>40f,34f</Size>
    <Children>
      <Template src="skin:knob.xml">
        <SetVariable name="group">[EffectRack<Variable name="EffectRack"/>_EffectUnit<Variable name="EffectUnit"/>_Effect<Variable name="Effect"/>]</SetVariable>
        <SetVariable name="control">meta</SetVariable>
        <SetVariable name="color">yellow</SetVariable>
      </Template>
    </Children>
  </WidgetGroup>
</Template>
