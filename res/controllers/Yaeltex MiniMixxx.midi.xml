      <output>
        <group>[Channel1]</group>
        <key>play_indicator</key>
        <status>0x90</status>
        <midino>0x0C</midino>
        <on>0x7f</on>
        <minimum>0.1</minimum>
      </output>

      <output>
        <group>[Channel2]</group>
        <key>play_indicator</key>
        <status>0x90</status>
        <midino>0x0F</midino>
