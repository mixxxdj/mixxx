        <group>[EffectRack1_EffectUnit1]</group>
        <key>enabled</key>
        <status>0x92</status>
        <midino>0x29</midino>
        <minimum>0.1</minimum>
      </output>
	  <output>
        <group>[EffectRack1_EffectUnit2]</group>
        <key>enabled</key>
        <status>0x92</status>
